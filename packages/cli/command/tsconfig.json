      "outDir": "dist",
      "strict": true,
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  